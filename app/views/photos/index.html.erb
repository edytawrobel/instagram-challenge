<% if @photos.all.any? %>
  <% @photos.all.each do |photo| %>
    <a href="<%= photo_path(photo) %>" class="image">
      <%= image_tag photo.image.url(:large) %>
    </a>
    <p><%= photo.status %></p>
  <% end %>
<% else %>
  <h1>No photos yet!</h1>
<% end %>

<%= link_to "New Photo", new_photo_path %>
